# MonarchMoney credentials for testing
# Copy this file to .env and fill in your actual credentials

# Required: Your MonarchMoney email and password
MONARCH_EMAIL=your-email@example.com
MONARCH_PASSWORD=your-password

# Optional: MFA secret key (if you have 2FA enabled)
# Get this from MonarchMoney settings -> Security -> Enable MFA -> "Two-factor text code"
MONARCH_MFA_SECRET=your-mfa-secret-key

# Optional: Force fresh login instead of using cached session
MONARCH_FORCE_LOGIN=false

# Optional: Enable debug logging
DEBUG=false